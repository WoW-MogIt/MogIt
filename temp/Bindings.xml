<Bindings>
	<Binding category="ADDONS" name="MogIt" header="MogIt">
		MogIt:ToggleFrame()
	</Binding>
	<Binding category="ADDONS" name="MogItPreview">
		MogIt:TogglePreview()
	</Binding>
	<Binding name="MogIt_TooltipScrollUp" hidden="true">
		MogIt.tooltip.model:SetFacing(MogIt.tooltip.model:GetFacing()+0.3);
	</Binding>
	<Binding name="MogIt_TooltipScrollDown" hidden="true">
		MogIt.tooltip.model:SetFacing(MogIt.tooltip.model:GetFacing()-0.3);
	</Binding>
</Bindings>